{
  "name": "@base-web-kits/base-tools-uni",
  "version": "0.8.9",
  "sideEffects": false,
  "description": "Independent Uni-app utilities package built from src/uni.",
  "keywords": [
    "base-tools",
    "uni",
    "utilities",
    "thin-wrapper"
  ],
  "license": "MIT",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./index": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./index.js": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./dist/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ],
      "index": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {},
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}

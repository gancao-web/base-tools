{
  "name": "@base-web-kits/base-tools-react",
  "version": "0.8.7",
  "type": "module",
  "sideEffects": false,
  "description": "Independent React utilities package built from src/react.",
  "keywords": [
    "base-tools",
    "react",
    "utilities",
    "thin-wrapper"
  ],
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.ts",
        "default": "./dist/index.d.ts"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ],
      "index": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "ahooks": "^3.9.6"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
